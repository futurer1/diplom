							Browser.deleteRoute(orient_interpol_ext_ramka, 'value_changed', ext_ramka, 'rotation');
							
							Browser.deleteRoute(timesens_for_int_ramka, 'fraction_changed', orient_interpol_rotor1, 'set_fraction');
